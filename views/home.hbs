    <!--materialize init-->
    <script>
      
      $('document').ready(() => {
        // sidebar init
        $('.sidenav').sidenav();
        // courasol
        $('.carousel.carousel-slider').carousel({
          fullWidth: true,
          indicators: true
        });

        setInterval(function() {
          $('.carousel').carousel('next');
        }, 4000);

        $('#join-us').on('mouseenter', function() {
          $('.join').addClass('pulse');
        });

        $('#join-us').on('mouseleave', function() {
          $('.join').removeClass('pulse');
        });

      });
      
    </script>
    
